{
  "name": "fox-react-builder-template",
  "version": "1.7.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "start": "webpack serve --progress --open --config ./configs/webpack.dev.js",
    "test": "jest",
    "coverage": "start ./coverage/lcov-report/index.html",
    "build": "webpack --progress --config ./configs/webpack.prod.js",
    "build:size": "webpack --progress --env size --config ./configs/webpack.prod.js",
    "build:speed": "webpack --progress --env speed --config ./configs/webpack.prod.js",
    "serve": "start http://localhost:5428 & npx serve -l 5428 -s dist ",
    "lint": "prettier --write",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "release": "standard-version",
    "release:version": "npm run release -- --release-as marjor",
    "release:feat": "npm run release -- --release-as minor",
    "release:bug": "npm run release -- --release-as patch"
  },
  "lint-staged": {
    "src/*.{js,jsx,mjs,ts,tsx}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/*.{css,scss,less,json,html,md,markdown}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 2 version",
    ">1%",
    "ios 7"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^13.1.8",
    "autoprefixer": "^9.0.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.3",
    "compression-webpack-plugin": "^7.1.2",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.3",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-transform-stub": "^2.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.4.0",
    "postcss-loader": "^5.2.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.1",
    "purgecss-webpack-plugin": "^4.0.3",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "^17.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.3.0",
    "thread-loader": "^3.0.1",
